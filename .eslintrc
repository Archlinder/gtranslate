{
    "rules": {
        "indent": [ 2, 2 ],
        "quotes": [ 2, "single", "avoid-escape" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "never" ],
        "comma-dangle": [ 2, "always-multiline" ],
        "camelcase": [ 2, { "properties": "never" } ],
        "spaced-comment": [ 2, "always" ],
        "space-infix-ops": [ 2 ]
    },
    "env": {
        "es6": true
    },
    "extends": "eslint:recommended"
}
